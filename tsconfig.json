{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "outDir": "build",
    "resolveJsonModule": true,
    "module": "commonjs",
    "lib": [
      "es5",
      "es6",
      "es2015.collection",
      "es2015.iterable",
      "es2015.core",
      "dom"
    ],
    "types": [
      "node",
      "jest"
    ],
    "target": "es2015",
    "strict": false,
    "allowJs": true
  },
  "include": [
    "src/com/google/common/css/*.js",
    "src/index.ts"
  ],
}
